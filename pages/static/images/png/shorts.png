<script type="text/javascript">	
	$('#comments_body').delegate('.js-comments-more', 'click', function(e) {		
		e.preventDefault();		
		$('#comments_body').fadeOut('slow');		
		$("#comments_body").load($(this).attr('data-href'));			
		$('#comments_body').fadeIn('slow');	
	});	
	
	$('#comments_body').load('index.php?route=information/articles/comments&articles_id=174');	
	
	$("#comments").delegate('.submitAjax', 'click', function() {
	
		var form = $('#jsform-comment');		
			
		ajaxForm(form);        
		return false	
	});			
		
	$("#comments").delegate('.jsform-comment', 'submit', function() {
	
		var form = $(this);		
			
		ajaxForm(form);        
		return false	
	});			
	
	function ajaxForm(form) {
		$.ajax({			
			type: "POST",			
			url: form.attr("action"),			
			dataType: "json",			
			data: form.serialize(),			
			success:function (json, f) {
				form.find(".form__message").empty();				
				form.find(".form__error").removeClass('form__error');		
				if (f === "success" && json['errors'] == false) {		
					$("#modal_success").find('.form__title > span').html(json['title']);	
					$("#modal_success").find('.form__about').html(json['success']);		
					$("#modal_success").foundation('open');		
					form.trigger('reset');	
					
					Dropzone.forElement("#myDropzone").files = [];
					$('div.dz-preview').remove();
					$('#myDropzone').removeClass('dz-started');
					form.find('input[name="images[]"]').remove();
					
					$("#parent_id").val(0);	
					$('#comments_body').load('index.php?route=information/articles/comments&articles_id=174');				
				} else {					
					for(var key in json) {					
						if (key != 'errors') {			
							form.find(".form__message." + key).parent().addClass('form__error');
							form.find(".form__message." + key).html(json[key]);
							form.find(".form__message." + key).show();
						}					
					}				
				}			
			}							
		});
	}
	
</script>


